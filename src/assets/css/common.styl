@charset "utf-8";
.clearfix:after {
  content: "\200B";
  display: block;
  height: 0;
  clear: both;
}
.wrap {
  margin-left: auto;
  margin-right: auto;
  width: 1200px;
}
.head-title{
  display: inline-block;
  float: left;
  margin-top: 8px;
  padding-left: 10px;
  min-width: 6em;
  border-left: 4px solid blue;
  font-size: 16px;
  line-height: 1.4;
}