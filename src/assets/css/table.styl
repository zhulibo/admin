.table-head {
  .head-title{
    margin-top: 18px;
  }
  .sch {
    float: left
    display: inline-block;
    margin-left: 30px
  }
  .new-btn {
    margin-top: 10px
    margin-left: 10px
    float right
  }
}
.sch input.el-input__inner {
  background-color: #fff
}
.table-form-inline .el-form-item {
  margin-top: 10px
  margin-bottom: 0
}
.table-form-inline .el-form-item__content .el-select,
.table-form-inline .el-form-item__content .el-input {
  width: 140px
}
.table {
  margin-top: 15px
  padding: 10px 20px 15px
  background-color: #fff
  box-shadow 0 0 3px rgba(0, 0, 0, .1)
}
.copy-span{
  cursor: pointer
}
.el-tooltip__popper {
  max-width 800px
}
.row-img img {
  height: 3em
}
.row-manage .el-button,
.row-manage .el-dropdown {
  padding-left: 5px
  padding-right: 5px
}
.el-button + .el-dropdown {
  margin-left: 10px
}
.row-manage .el-button.edit:hover
.row-manage .el-button.detail:hover {
  color: blue-hover
}
.row-manage .el-button.delete {
  color: #999
}
.row-manage .el-button.delete:hover {
  color: red
}
.pagination-ct {
  overflow: hidden
  margin-top: 20px
  .manage-batch {
    float: left
    display: inline-block
  }
  .el-pagination {
    float: right
    display: inline-block
  }
}
.pagination-ct .el-pagination.is-background .el-pager li {
}
